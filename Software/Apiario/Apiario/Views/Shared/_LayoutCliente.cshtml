@if(Session["clienteLogadoID"] == null && Session["adminLogadoID"] == null || Session["adminLogadoID"] != null)
{
    Response.Redirect("Logar");
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Apiário</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Cliente", "Index", "Cliente", null, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Início", "Index", "Cliente")</li>
                    <li>@Html.ActionLink("Visualizar Apiário", "VisualizarApiario", "Cliente")</li>
                    <li>@Html.ActionLink("Remover Apiário", "RemoverApiario", "Cliente")</li>
                    <li>@Html.ActionLink("Cadastrar Apiário", "CadastrarApiario", "Cliente")</li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a>@Session["clienteLogadoID"]</a></li>
                    <li>@Html.ActionLink("Deslogar", "Deslogar", "Cliente")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p class="text-center">&copy; @DateTime.Now.Year - Apiário</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/inputmask")
    @RenderSection("scripts", required: false)
</body>
</html>